{
    "name": "transformstudios/uptime",
    "autoload": {
        "psr-4": {
            "TransformStudios\\Uptime\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TransformStudios\\Uptime\\Tests\\": "tests"
        }
    },
    "require": {
        "php": "^8.0",
        "laravel/framework": "^9.9",
        "statamic/cms": "^3.4",
        "transformstudios/front": "^1.12.1"
    },
    "require-dev": {
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^6.0",
        "phpunit/phpunit": "^9.0",
        "orchestra/testbench": "^7.0",
        "spatie/laravel-ray": "^1.24"
    },
    "extra": {
        "statamic": {
            "name": "Uptime",
            "description": "Uptime addon"
        },
        "laravel": {
            "providers": [
                "TransformStudios\\Uptime\\ServiceProvider"
            ]
        },
        "patches": {}
    },
    "config": {
        "allow-plugins": {
            "pixelfear/composer-dist-plugin": true
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/transformstudios/statamic-front"
        }
    ]
}
